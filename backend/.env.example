# Configuration de l'application TrouveTonPadel
# Renommez ce fichier en .env et complétez les valeurs

# Port du serveur (optionnel, par défaut 3000)
PORT=3000

# Identifiants P4 Padel
P4_EMAIL_1=votre_email_1@exemple.com
P4_EMAIL_2=votre_email_2@exemple.com
P4_EMAIL_3=votre_email_3@exemple.com

# Durée du cache en minutes (optionnel, par défaut 5)
CACHE_DURATION_MINUTES=5
